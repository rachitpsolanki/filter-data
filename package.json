{
  "name": "filter-data",
  "version": "0.1.1",
  "description": "simple, fast data filter",
  "keywords": [
    "match-sorter",
    "data filter",
    "filter data",
    "search data",
    "datatable",
    "fast search"
  ],
  "author": "ThunderMiracle",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundermiracle/filter-data.git"
  },
  "bugs": {
    "url": "https://github.com/thundermiracle/filter-data/issues"
  },
  "homepage": "https://github.com/thundermiracle/filter-data#readme",
  "scripts": {
    "eslint:src": "eslint src/**/*.{ts,tsx}",
    "eslint:test": "eslint __test__/**/*.{ts,tsx}",
    "eslint": "yarn eslint:src && yarn eslint:test",
    "jest": "jest",
    "type-check": "tsc --noEmit --project tsconfig.eslint.json",
    "type-check:watch": "yarn type-check -- --watch",
    "test": "yarn type-check && yarn eslint && yarn jest && codecov",
    "bench": "ts-node ./benchmark/index.ts",
    "build:js:module": "tsc",
    "build:js:es": "tsc --project tsconfig.es.json",
    "build:js": "yarn build:js:module && yarn build:js:es",
    "webpack:dev": "webpack --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "bundle": "yarn webpack:dev && yarn webpack:prod",
    "prebuild": "rimraf lib/ && rimraf dist/ && rimraf es/",
    "build": "yarn build:js && yarn bundle"
  },
  "dependencies": {
    "ramda": "0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/types": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@types/match-sorter": "^4.0.0",
    "@types/node": "^13.7.6",
    "@types/ramda": "^0.26.42",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "fuse.js": "^3.4.6",
    "jest": "^25.1.0",
    "match-sorter": "^4.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
